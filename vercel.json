{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "headers": [
    {
      "source": "/ws",
      "headers": [
        { "key": "Upgrade", "value": "websocket" },
        { "key": "Connection", "value": "Upgrade" }
      ]
    }
  ]
}